{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["*"],
      "@shared/*": [
        "./shared/*"
      ],
      "@api/*": [
        "./api/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@app/*": [
        "./app/*"
      ]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
